    "react": "^19.2.3",
    "react-dom": "^18.2.0",
    "react-easy-crop": "^5.5.6",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.2.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "jsdom": "^21.1.2",
    "pa11y": "^9.0.1",
    "postcss": "^8.0.0",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.0.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.1.6",
    "vite": "^7.3.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,md,css,html}": [
      "prettier --write"
    ]
  },
  "volta": {
    "node": "20.19.0"
  }
}
